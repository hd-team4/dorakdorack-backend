<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dorakdorak.domain.member.mapper.InitMapper">

  <select id="findUniversities" resultType="dorakdorak.domain.member.dto.response.InitDataResponse">
    SELECT
    ID,
    NAME
    FROM UNIVERSITY
  </select>

  <select id="findAllergys" resultType="dorakdorak.domain.member.dto.response.InitDataResponse">
    SELECT
    ID,
    NAME
    FROM ALLERGY_CATEGORY
  </select>
</mapper>

